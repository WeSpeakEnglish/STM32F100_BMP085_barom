><td class="source">void i2c_ev_handler(void) {<br></td></tr
><tr
id=sl_svn136_145

><td class="source">  static uint8_t subaddress_sent, final_stop; //flag to indicate if subaddess sent, flag to indicate final bus condition<br></td></tr
><tr
id=sl_svn136_146

><td class="source">  static int8_t index;        //index is signed -1==send the subaddress<br></td></tr
><tr
id=sl_svn136_147

><td class="source">  uint8_t SReg_1 = I2Cx-&gt;SR1; //read the status register here<br></td></tr
><tr
id=sl_svn136_148

><td class="source">  if (SReg_1 &amp; 0x0001) {      //we just sent a start - EV5 in ref manual<br></td></tr
><tr
id=sl_svn136_149

><td class="source">    I2Cx-&gt;CR1 &amp;= ~0x0800;  //reset the POS bit so ACK/NACK applied to the current byte<br></td></tr
><tr
id=sl_svn136_150

><td class="source">    I2C_AcknowledgeConfig(I2Cx, ENABLE);    //make sure ACK is on<br></td></tr
><tr
id=sl_svn136_151

><td class="source">    index = 0;              //reset the index<br></td></tr
><tr
id=sl_svn136_152

><td class="source">    if (reading &amp;&amp; (subaddress_sent || 0xFF == reg)) {       //we have sent the subaddr<br></td></tr
><tr
id=sl_svn136_153

><td class="source">      subaddress_sent = 1;        //make sure this is set in case of no subaddress, so following code runs correctly<br></td></tr
><tr
id=sl_svn136_154

><td class="source">      if (bytes == 2)<br></td></tr
><tr
id=sl_svn136_155

><td class="source">        I2Cx-&gt;CR1 |= 0x0800;    //set the POS bit so NACK applied to the final byte in the two byte read<br></td></tr
><tr
id=sl_svn136_156

><td class="source">      I2C_Send7bitAddress(I2Cx, addr, I2C_Direction_Receiver);   //send the address and set hardware mode<br></td></tr
><tr
id=sl_svn136_157

><td class="source">    } else {                //direction is Tx, or we havent sent the sub and rep start<br></td></tr
><tr
id=sl_svn136_158

><td class="source">      I2C_Send7bitAddress(I2Cx, addr, I2C_Direction_Transmitter);        //send the address and set hardware mode<br></td></tr
><tr
id=sl_svn136_159

><td class="source">      if (reg != 0xFF)       //0xFF as subaddress means it will be ignored, in Tx or Rx mode<br></td></tr
><tr
id=sl_svn136_160

><td class="source">        index = -1;     //send a subaddress<br></td></tr
><tr
id=sl_svn136_161

><td class="source">    }<br></td></tr
><tr
id=sl_svn136_162

><td class="source">  } else if (SReg_1 &amp; 0x0002) {       //we just sent the address - EV6 in ref manual<br></td></tr
><tr
id=sl_svn136_163

><td class="source">    //Read SR1,2 to clear ADDR<br></td></tr
><tr
id=sl_svn136_164

><td class="source">    volatile uint8_t a;<br></td></tr
><tr
id=sl_svn136_165

><td class="source">    __DMB(); // memory fence to control hardware<br></td></tr
><tr
id=sl_svn136_166

><td class="source">    if (bytes == 1 &amp;&amp; reading &amp;&amp; subaddress_sent) { //we are receiving 1 byte - EV6_3<br></td></tr
><tr
id=sl_svn136_167

><td class="source">      I2C_AcknowledgeConfig(I2Cx, DISABLE);       //turn off ACK<br></td></tr
><tr
id=sl_svn136_168

><td class="source">      __DMB();<br></td></tr
><tr
id=sl_svn136_169

><td class="source">      a = I2Cx-&gt;SR2;      //clear ADDR after ACK is turned off<br></td></tr
><tr
id=sl_svn136_170

><td class="source">      I2C_GenerateSTOP(I2Cx, ENABLE);     //program the stop<br></td></tr
><tr
id=sl_svn136_171

><td class="source">      final_stop = 1;<br></td></tr
><tr
id=sl_svn136_172

><td class="source">      I2C_ITConfig(I2Cx, I2C_IT_BUF, ENABLE);     //allow us to have an EV7<br></td></tr
><tr
id=sl_svn136_173

><td class="source">    } else {                //EV6 and EV6_1<br></td></tr
><tr
id=sl_svn136_174

><td class="source">      a = I2Cx-&gt;SR2;      //clear the ADDR here<br></td></tr
><tr
id=sl_svn136_175

><td class="source">      __DMB();<br></td></tr
><tr
id=sl_svn136_176

><td class="source">      if (bytes == 2 &amp;&amp; reading &amp;&amp; subaddress_sent) {     //rx 2 bytes - EV6_1<br></td></tr
><tr
id=sl_svn136_177

><td class="source">        I2C_AcknowledgeConfig(I2Cx, DISABLE);   //turn off ACK<br></td></tr
><tr
id=sl_svn136_178

><td class="source">        I2C_ITConfig(I2Cx, I2C_IT_BUF, DISABLE);        //disable TXE to allow the buffer to fill<br></td></tr
><tr
id=sl_svn136_179

><td class="source">      } else if (bytes == 3 &amp;&amp; reading &amp;&amp; subaddress_sent)       //rx 3 bytes<br></td></tr
><tr
id=sl_svn136_180

><td class="source">        I2C_ITConfig(I2Cx, I2C_IT_BUF, DISABLE);        //make sure RXNE disabled so we get a BTF in two bytes time<br></td></tr
><tr
id=sl_svn136_181

><td class="source">      else                //receiving greater than three bytes, sending subaddress, or transmitting<br></td></tr
><tr
id=sl_svn136_182

><td class="source">        I2C_ITConfig(I2Cx, I2C_IT_BUF, ENABLE);<br></td></tr
><tr
id=sl_svn136_183

><td class="source">    }<br></td></tr
><tr
id=sl_svn136_184

><td class="source">  } else if (SReg_1 &amp; 0x004) {        //Byte transfer finished - EV7_2, EV7_3 or EV8_2<br></td></tr
><tr
id=sl_svn136_185

><td class="source">    final_stop = 1;<br></td></tr
><tr
id=sl_svn136_186

><td class="source">    if (reading &amp;&amp; subaddress_sent) {     //EV7_2, EV7_3<br></td></tr
><tr
id=sl_svn136_187

><td class="source">      if (bytes &gt; 2) {      //EV7_2<br></td></tr
><tr
id=sl_svn136_188

><td class="source">        I2C_AcknowledgeConfig(I2Cx, DISABLE);   //turn off ACK<br></td></tr
><tr
id=sl_svn136_189

><td class="source">        read_p[index++] = I2C_ReceiveData(I2Cx);    //read data N-2<br></td></tr
><tr
id=sl_svn136_190

><td class="source">        I2C_GenerateSTOP(I2Cx, ENABLE); //program the Stop<br></td></tr
><tr
id=sl_svn136_191

><td class="source">        final_stop = 1; //reuired to fix hardware<br></td></tr
><tr
id=sl_svn136_192

><td class="source">        read_p[index++] = I2C_ReceiveData(I2Cx);    //read data N-1<br></td></tr
><tr
id=sl_svn136_193

><td class="source">        I2C_ITConfig(I2Cx, I2C_IT_BUF, ENABLE); //enable TXE to allow the final EV7<br></td></tr
><tr
id=sl_svn136_194

><td class="source">      } else {            //EV7_3<br></td></tr
><tr
id=sl_svn136_195

><td class="source">        if (final_stop)<br></td></tr
><tr
id=sl_svn136_196

><td class="source">          I2C_GenerateSTOP(I2Cx, ENABLE);     //program the Stop<br></td></tr
><tr
id=sl_svn136_197

><td class="source">        else<br></td></tr
><tr
id=sl_svn136_198

><td class="source">          I2C_GenerateSTART(I2Cx, ENABLE);    //program a rep start<br></td></tr
><tr
id=sl_svn136_199

><td class="source">        read_p[index++] = I2C_ReceiveData(I2Cx);    //read data N-1<br></td></tr
><tr
id=sl_svn136_200

><td class="source">        read_p[index++] = I2C_ReceiveData(I2Cx);    //read data N<br></td></tr
><tr
id=sl_svn136_201

><td class="source">        index++;        //to show job completed<br></td></tr
><tr
id=sl_svn136_202

><td class="source">      }<br></td></tr
><tr
id=sl_svn136_203

><td class="source">    } else {                //EV8_2, which may be due to a subaddress sent or a write completion<br></td></tr
><tr
id=sl_svn136_204

><td class="source">      if (subaddress_sent || (writing)) {<br></td></tr
><tr
id=sl_svn136_205

><td class="source">        if (final_stop)<br></td></tr
><tr
id=sl_svn136_206

><td class="source">          I2C_GenerateSTOP(I2Cx, ENABLE);     //program the Stop<br></td></tr
><tr
id=sl_svn136_207

><td class="source">        else<br></td></tr
><tr
id=sl_svn136_208

><td class="source">          I2C_GenerateSTART(I2Cx, ENABLE);    //program a rep start<br></td></tr
><tr
id=sl_svn136_209

><td class="source">        index++;        //to show that the job is complete<br></td></tr
><tr
id=sl_svn136_210

><td class="source">      } else {            //We need to send a subaddress<br></td></tr
><tr
id=sl_svn136_211

><td class="source">        I2C_GenerateSTART(I2Cx, ENABLE);        //program the repeated Start<br></td></tr
><tr
id=sl_svn136_212

><td class="source">        subaddress_sent = 1;    //this is set back to zero upon completion of the current task<br></td></tr
><tr
id=sl_svn136_213

><td class="source">      }<br></td></tr
><tr
id=sl_svn136_214

><td class="source">    }<br></td></tr
><tr
id=sl_svn136_215

><td class="source">    //we must wait for the start to clear, otherwise we get constant BTF<br></td></tr
