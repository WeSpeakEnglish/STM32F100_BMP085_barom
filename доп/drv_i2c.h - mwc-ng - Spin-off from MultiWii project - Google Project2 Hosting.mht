}
.pfeatured {
  background-color:#ddf8cc;
}
.psmeta {
  margin: 0 5px 0 0;
  padding: 0;
}
.psline {
  height: 1px;
  border: 0;
  background-color:#e9e9e9;
  margin:5px 5px 10px 10px;
}
.psgap {
  height: 1em;
}
.nowrap {
  white-space:nowrap;
}
#derived_labels_target .label {font-weight: bold;}
#derived_labels_target .label:hover {text-decoration: none;}

